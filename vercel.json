{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(Brisbane|Adelaide|Sydney|Melbourne|Perth|Darwin|Hobart|Canberra)/(.*)", "dest": "index.js" },
    { "src": "/api/(.*)", "dest": "index.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
